{
  "name": "@loopback/grpc",
  "version": "0.2.0",
  "description": "A gRPC extension for LoopBack Next",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^10.16 || 12 || 14"
  },
  "bin": {
    "lb-grpc": "node dist/grpc.code-gen"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean loopback-grpc*.tgz dist package api-docs *.tsbuildinfo",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/grpc"
  },
  "author": "IBM Corp.",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "compilers",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.1.6",
    "@grpc/proto-loader": "^0.5.5",
    "debug": "^4.1.1",
    "glob": "^7.1.6",
    "minimist": "^1.2.5",
    "protobufjs": "^6.10.1"
  },
  "peerDependencies": {
    "@loopback/boot": "^3.0.2",
    "@loopback/core": "^2.11.0"
  },
  "devDependencies": {
    "@loopback/boot": "^3.0.2",
    "@loopback/build": "^6.2.5",
    "@loopback/core": "^2.11.0",
    "@loopback/eslint-config": "^10.0.1",
    "@loopback/testlab": "^3.2.7",
    "@types/debug": "^4.1.5",
    "@types/glob": "^7.1.2",
    "@types/minimist": "^1.2.0"
  },
  "copyright.owner": "IBM Corp.",
  "publishConfig": {
    "access": "public"
  }
}
